<h2>プロフィール編集</h2>
<%= form_with model:@user, local:true do |f| %>
  <p>プロフィール画像 <%= attachment_image_tag @user, :profile_image, :fill, 60, 60, fallback: "no_image.jpg"%>
  <%= f.attachment_field :profile_image, placeholder: "プロフィール画像" %></p>
  <p>ニックネーム <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前"%></p>
  <p>自己紹介文 <%= f.text_area :self_introduction, autofocus: true %></p>
  <%= f.submit "変更を保存" %>
   <%= link_to "退会ページ", unsubscribe_path %>
<% end %>