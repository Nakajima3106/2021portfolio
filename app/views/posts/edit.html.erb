<h1>編集画面</h1>
<!-- form_with部分 -->
<%= form_with model:@post, local: true do |f| %>
  <h4>画像</h4>
  <%= f.file_field :image %>
  <h4>説明文</h4>
  <%= f.text_area :explanation, rows:'5',placeholder: "お酒の種類や名前,どこで飲んでいるのか,誰と飲んでいるのか,などなど"%>
  <h4>感想文</h4>
  <%= f.text_area :impressions, rows:'5' %>
  <%= f.submit '保存' %>
<% end %>